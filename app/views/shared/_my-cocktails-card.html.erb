<i class="fas fa-edit"></i>
  <div class="cards">
    <% @cocktails_results.each do |cocktail| %>
      <div class="card-cocktails" data-aos="fade-in" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path cocktail.photo.key, height: 1200, width: 1200, crop: :fill %>');">
        <%=link_to cocktail.name, cocktail_path(cocktail)%>
      </div>
    <% end %>
  </div>
